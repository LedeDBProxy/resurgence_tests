#  测试用例的目的：
#     随机性测试，用来测试非prepare连接,在正常情况下的连接复用情况
#

#set up parameters

let $ip= $PROXY_HOST;
let $admin_ip= $PROXY_HOST;
let $port= $PROXY_PORT;
let $admin_port= $PROXY_ADMIN_PORT;
let $user= $MYSQL_USER;
let $pwd= $MYSQL_PASSWORD;
let $proxy_admin_pwd= $PROXY_ADMIN_PASSWORD;

temp_disable_ps_protocol;
#create connection for administration
connect (conn0,$admin_ip,$user,$proxy_admin_pwd,,$admin_port); 
#assume 1
eval select conn_num from backends where backend_ndx = 1 and user = "$user";

#create connection and test


connect (conn1,$ip,$user,$pwd,,$port); 
character_set utf8;
use test;
set @a=1;
set autocommit=0;
SELECT data from user where id = @a;
set @a=2;
SELECT data from user where id = @a;
set @a=3;
SELECT id, data from user where id = @a;
rollback;
set autocommit=1;
set @a=4;
SELECT id, data from user where id = @a;

real_sleep 0.5;
connection conn0;
#assume 2
eval select conn_num from backends where backend_ndx = 1 and user = "$user";

real_sleep 0.5;
connect (conn2,$ip,$user,$pwd,,$port); 
use test;
set @a=2;
SELECT data from user where id = @a;

real_sleep 0.5;
connect (conn3,$ip,$user,$pwd,,$port); 
use test;
set @a=3;
SELECT data from user where id = @a;

real_sleep 0.5;
connect (conn4,$ip,$user,$pwd,,$port);
use test;
set @a=4;
SELECT data from user where id = @a;

real_sleep 0.5;
connection conn0;
#assume 5
eval select conn_num from backends where backend_ndx = 1 and user = "$user";

real_sleep 0.5;
connect (conn5,$ip,$user,$pwd,,$port);
use test;
set @a=6;
SELECT data from user where id = @a;

real_sleep 0.5;
connection conn0;
#assume 5
eval select conn_num from backends where backend_ndx = 1 and user = "$user";

real_sleep 0.5;
connect (conn6,$ip,$user,$pwd,,$port);
use test;
set @a=6;
SELECT data from user where id = @a;

real_sleep 0.5;
connection conn0;
#assume 5
eval select conn_num from backends where backend_ndx = 1 and user = "$user";

connect (conn7,$ip,$user,$pwd,,$port);
use test;
set @a=6;
SELECT data from user where id = @a;

connect (conn8,$ip,$user,$pwd,,$port);
use test;
set @a=6;
SELECT data from user where id = @a;

connect (conn9,$ip,$user,$pwd,,$port);
use test;
set @a=6;
SELECT data from user where id = @a;

connect (conn10,$ip,$user,$pwd,,$port);
use test;
set @a=6;
SELECT data from user where id = @a;

real_sleep 0.5;
connection conn0;
#assume 5
eval select conn_num from backends where backend_ndx = 1 and user = "$user";

disconnect conn1;
disconnect conn2;
disconnect conn3;
disconnect conn4;
disconnect conn5;
disconnect conn9;
disconnect conn8;
disconnect conn7;
disconnect conn6;
disconnect conn10;

real_sleep 0.5;
connection conn0;
#assume 5
eval select conn_num from backends where backend_ndx = 1 and user = "$user";

disconnect conn0;
